# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_ANON_KEY=your-anon-key

# OpenAI
OPENAI_API_KEY=sk-your-key

# Apify (Twitter scraping)
APIFY_API_TOKEN=apify_api_your-token

# YouTube
YOUTUBE_API_KEY=your-youtube-api-key

# Resend (email)
RESEND_API_KEY=re_your-key

# Email config
DIGEST_RECIPIENT_EMAIL=you@example.com
DIGEST_FROM_EMAIL=digest@yourdomain.com

# Feedback API base URL (where FastAPI is deployed)
FEEDBACK_API_URL=https://your-app.onrender.com

# Twitter list URLs (comma-separated)
TWITTER_LIST_URLS=https://twitter.com/i/lists/123456789

# RSS feed URLs (comma-separated)
RSS_FEED_URLS=https://example.com/feed.xml,https://another.com/rss

# YouTube channel IDs (comma-separated)
YOUTUBE_CHANNEL_IDS=UCxxxxxxxxxxxxxx

# Streamlit context update URL
STREAMLIT_APP_URL=https://your-app.streamlit.app
